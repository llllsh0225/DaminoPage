<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="MapDAO">
	
	<select id="getStoreList" resultType="com.damino.web.user.map.MapVO">
		SELECT * FROM daminostoreinfo
	</select>
	
	<select id="getSearchStoreList" parameterType="String" resultType="String">
		SELECT storeName FROM daminostoreinfo
		WHERE storeRegion = #{storeRegion}
	</select>
	
</mapper>
